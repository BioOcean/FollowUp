<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<packageSources>
		<!-- 默认的公共源 -->
		<add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
		<add key="MyLocalNuGetFeed" value="http://222.128.24.148:5555/v3/index.json" allowInsecureConnections="True" />
	</packageSources>

	<!-- 包源映射 -->
	<packageSourceMapping>
		<packageSource key="nuget.org">
			<package pattern="*" />
		</packageSource>
		<packageSource key="MyLocalNuGetFeed">
			<package pattern="Bio.*" />
		</packageSource>
	</packageSourceMapping>

	<!-- 确保启用了所有源 -->
	<activePackageSource>
		<add key="All" value="(Aggregate source)" />
	</activePackageSource>
</configuration>
